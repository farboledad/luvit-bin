	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION
		"RF Channel"
	::= { primaryThreeGInterface 15 }

plmn OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION
		"PLMN"
	::= { primaryThreeGInterface 16 }

csCellIDandLac OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION
		"CS Cell ID and LAC"
	::= { primaryThreeGInterface 18 }

psCellIDandLac OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION
		"PS Cell ID and LAC"
	::= { primaryThreeGInterface 19 }

pinStatus OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION
		"PIN status"
	::= { primaryThreeGInterface 20 }

