-- OPTION-CLOUDGATE.mib - MIB for private Option CloudGate management objects.

OPTION-CLOUDGATE-MIB DEFINITIONS ::= BEGIN

IMPORTS

	enterprises, IpAddress
		FROM RFC1155-SMI
	DisplayString
		FROM RFC1213-MIB
	OBJECT-TYPE
		FROM RFC-1212;

option OBJECT IDENTIFIER
	::= { enterprises 21415 }

cloudGatePrivate OBJECT IDENTIFIER 
	::= { option 0 }

systemInfo OBJECT IDENTIFIER ::= { cloudGatePrivate 0 }
primaryThreeGInterface OBJECT IDENTIFIER ::= { cloudGatePrivate 1 }
wlanAccesspoint OBJECT IDENTIFIER ::= { cloudGatePrivate 2 }
gpslocation OBJECT IDENTIFIER ::= { cloudGatePrivate 3 }

upTime OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"System up time"

	::= { systemInfo 0 }

hostName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"Hostname"

	::= { systemInfo 1 }

currentTime OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"Current Time"

	::= { systemInfo 2 }

serialNumber OBJECT-TYPE
	SYNTAX Integer32
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"Serial Number of CloudGate"

	::= { systemInfo 3 }

sysLoad1 OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"System load for the last minute"

	::= { systemInfo 4 }

sysLoad5 OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"System load for the last 5 minutes"

	::= { systemInfo 5 }

sysLoad15 OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"System load for the last 15 minutes"

	::= { systemInfo 6 }

firmVer OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS current
        DESCRIPTION
                "CloudGate's firmware version"

        ::= { systemInfo 7 }

AssociatedClients OBJECT-TYPE
    SYNTAX          Gauge32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Number of clients currently associated with the access point" 
    ::= { wlanAccesspoint 0 }

ClientAssociations OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Historical number of client associations" 
    ::= { wlanAccesspoint 1 }

ClientDisassociations OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Historical number of client disassociations" 
    ::= { wlanAccesspoint 2 }

GPSstatus OBJECT-TYPE
    SYNTAX          INTEGER  {
                        gpsDisabled(1),
                        gpsAcquiring(2),
                        gpsEnabled(3),
                        gpsLocError(4)
                    }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Location base service state.

        gpsDisabled - GPS is disabled
        gpsEnabled  - GPS is enabled
        gpsLocError - GPS encounters error
        gpsAcquiring - GPS is acquiring fix"
    DEFVAL          { gpsDisabled } 
    ::= { gpslocation 0 }
	
Latitude OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Location base service Latitude in degrees" 
    ::= { gpslocation 1 }

Longitude OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Location base service longitude in degrees" 
    ::= { gpslocation 2 }

Elevation OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Antenna altitude above/below mean sea level (geoid) in meters" 
    ::= { gpslocation 3 }

Speed OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Speed over the ground in kilometers/hour" 
    ::= { gpslocation 4 }
	
Direction OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Track angle in degrees" 
    ::= { gpslocation 5 }
	
Declination OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Magnetic variation degrees (Easterly var. subtracts from true course)" 
    ::= { gpslocation 6 }

NmeaTime OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "UTC of position" 
    ::= { gpslocation 7 }
	
gpsPDOP OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Position Dilution Of Precision" 
    ::= { gpslocation 8 }

gpsHDOP OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Horizontal Dilution Of Precision" 
    ::= { gpslocation 9 }

gpsVDOP OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "Vertical Dilution Of Precision" 
    ::= { gpslocation 10 }
	
apn OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"Access Point Name"

	::= { primaryThreeGInterface 0 }

internetStatus OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"Internet Status"

	::= { primaryThreeGInterface 1 }

