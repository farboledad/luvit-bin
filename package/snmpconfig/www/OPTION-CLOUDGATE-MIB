	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"IP address"

	::= { primaryThreeGInterface 2 }

bCastAddress OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS current
        DESCRIPTION
                "Broadcast address"

        ::= { primaryThreeGInterface 3 }

netMask OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS current
        DESCRIPTION
                "Network Mask"

        ::= { primaryThreeGInterface 4 }

csRegistration OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"CS Registration"

	::= { primaryThreeGInterface 5 }	

psRegistration OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"PS Registration"

	::= { primaryThreeGInterface 6 }

