	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"PLMN"

	::= { primaryThreeGInterface 16 }

csCellIDandLac OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"CS Cell ID and LAC"

	::= { primaryThreeGInterface 18 }

psCellIDandLac OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"PS Cell ID and LAC"

	::= { primaryThreeGInterface 19 }

pinStatus OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"PIN status"

	::= { primaryThreeGInterface 20 }

pinRetries OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"PIN retries"

	::= { primaryThreeGInterface 21 }

pukRetries OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"PUK retries"

	::= { primaryThreeGInterface 22 }

rxPackets OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"RX Packets"

	::= { primaryThreeGInterface 23 }

rxBytes OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"RX Bytes"

	::= { primaryThreeGInterface 24 }

txPackets OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"TX Packets"

	::= { primaryThreeGInterface 25 }

txBytes OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"TX Bytes"

	::= { primaryThreeGInterface 26 }
	
END
