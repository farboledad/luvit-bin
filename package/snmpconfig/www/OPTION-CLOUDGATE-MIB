	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"IMEI"

	::= { primaryThreeGInterface 7 }

imsi OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"IMSI"

	::= { primaryThreeGInterface 8 }

firmMode OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS current
        DESCRIPTION
                "Firmware mode"

        ::= { primaryThreeGInterface 9 }

carrier OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"Carrier"

	::= { primaryThreeGInterface 10 }

rssi OBJECT-TYPE
	SYNTAX Integer32
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"RSSI"

	::= { primaryThreeGInterface 11 }

ecio OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"ECIO"

	::= { primaryThreeGInterface 12 }

bandClass OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"Band Class"

	::= { primaryThreeGInterface 13 }

serviceType OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS current
	DESCRIPTION 
		"Service type"

	::= { primaryThreeGInterface 14 }

